# Scout Analytics v3.2.0 Production Configuration
# Clean promotion from v2.1 with enhanced stability

# Core Settings
NODE_ENV=production
NEXT_PUBLIC_ENVIRONMENT=production
NEXT_PUBLIC_VERSION=3.2.0
NEXT_PUBLIC_SITE_URL=https://scout-mvp.vercel.app
NEXT_PUBLIC_PLATFORM_NAME=Scout Analytics

# Feature Flags - Production Ready
ENABLE_AI_AGENTS=true
ENABLE_SUPABASE=true
ENABLE_EXTERNAL_APIS=true
FALLBACK_MODE_AVAILABLE=true
SAFE_MODE_ENABLED=false

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_KEY=your-service-key

# AI Services
OPENAI_API_KEY=sk-your-openai-key
ANTHROPIC_API_KEY=your-anthropic-key
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/

# Deployment
VERCEL_TOKEN=your-vercel-token
VERCEL_PROJECT_ID=scout-mvp
VERCEL_ORG_ID=your-org-id

# QA & Monitoring
QA_MODE=production
AUDIT_LEVEL=full
ENABLE_KEYKEY=true
ENABLE_CACA=true

# Performance
NEXT_PUBLIC_API_TIMEOUT=30000
NEXT_PUBLIC_CACHE_TTL=300
NEXT_PUBLIC_MAX_RETRIES=3

# Error Handling
NEXT_PUBLIC_ERROR_BOUNDARY=true
NEXT_PUBLIC_FALLBACK_UI=true
SKIP_DATA_ON_ERROR=true